{
	"name": "Emojoy",
	"version": "0.0.1",
	"manifest_version": 2,
	"description": "Chromeを驚くほど使いづらくする拡張機能。毎日の作業にほんの少しのスパイスを。",
	"content_scripts": [
	  {
			"matches": ["http://*/*", "https://*/*"],
			"exclude_matches": ["https://www.google.com/search*"],
			"js": ["jquery-3.5.1.min.js", "tiny_segmenter-0.2.js", "fanc.js"],
			"run_at": "document_idle"
	  }
	]
}